pipeline {
    agent any

    stages {
        stage('Pull from GitHub') {
            steps {
                // Pull code from GitHub
                git branch: 'main', url: 'https://github.com/GhassenAbida/tp.git'
            }
        }

        stage('Build Docker Image') {
            steps {
                // Build Docker image
                sh 'docker build -t my-image .'
            }
        }

        }
}
